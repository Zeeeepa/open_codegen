# Codegen AI Proxy Environment Variables
# Copy this file to .env and fill in your values

# REQUIRED: Your Codegen API Key
CODEGEN_API_KEY=your-codegen-api-key-here

# OPTIONAL: Codegen API Base URL (default: https://api.codegen.com)
CODEGEN_BASE_URL=https://api.codegen.com

# OPTIONAL: Default system message for all requests
DEFAULT_SYSTEM_MESSAGE=You are a helpful AI assistant.

# OPTIONAL: Logging configuration
LOG_LEVEL=INFO
LOG_REQUESTS=true

# OPTIONAL: Server configuration
HOST=0.0.0.0
WORKERS=1
